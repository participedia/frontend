  "scripts": {
    "start": "concurrently \"sass --watch src\"  \"react-scripts start\"",
    "archiveprod": "zip -r archive/prod-$(date -u +%FT%TZ) build",
    "archivestage": "zip -r archive/stage-$(date -u +%FT%TZ) build",
    "deployprod": "surge -p build -d https://participedia.xyz",
    "deploystage": "surge -p build -d https://ppedia.surge.sh",
    "buildprod": "cp .envprod .env  && rm -rf build && sass --update src && react-scripts build && cp build/index.html build/200.html && node scripts/properties2json && echo $(date -u +%FT%TZ) > build/VERSION.txt && npm run archiveprod; cp .envdev .env",
    "buildstage": "cp .envstage .env && rm -rf build && sass --update src && react-scripts build && cp build/index.html build/200.html && node scripts/properties2json && echo $(date -u +%FT%TZ) > build/VERSION.txt && npm run archivestage; cp .envdev .env",
    "builddev": "rm -rf build && sass --update --force src && react-scripts build && cp build/index.html build/200.html && node scripts/properties2json && echo $(date -u +%FT%TZ) > build/VERSION.txt",
    "buildallprod": "npm install && sass --update --force src && npm run buildprod && npm test",
    "buildallstage": "npm install && sass --update --force src && npm run buildstage && npm test",
    "rollbackto": "f(){ rm -rf build && unzip archive/${1}; }; f",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint \"src/**/**.js\"",
    "lint-staged": "prettier --write",
    "analyze": "source-map-explorer build/static/js/main.* ",
    "eject": "react-scripts eject",
    "storybook": "sass --update src && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": "lint-staged"
}
